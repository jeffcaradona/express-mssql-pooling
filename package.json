    "start:dev:inspect": "cross-env NODE_ENV=development DEBUG=express-mssql-pooling:* nodemon --exec node --inspect ./src/server.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "bootstrap": "^5.3.8",
    "cookie-parser": "~1.4.7",
    "cors": "^2.8.5",
    "datatables.net-bs5": "^2.3.5",
    "debug": "~4.4.3",
    "dotenv": "^17.2.3",
    "dotenv-expand": "^12.0.3",
    "ejs": "^3.1.10",
    "express": "^5.2.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "http-errors": "~2.0.1",
    "morgan": "~1.10.1",
    "mssql": "^12.2.0",
    "winston": "^3.19.0"
  },
  "overrides": {"qs":"6.14.1"},
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "autocannon": "^8.0.0",
    "cross-env": "^10.1.0",
