{
  "name": "express-mssql-pooling",
  "description": "A simple Express 5 application with ES6 modules and TypeScript support.",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "files": [
    "src/",
    "views/",
    "public/",
    "package.json"
  ],
  "exports": "./src/app.js",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "start": "node ./src/server.js",
    "start:dev": "cross-env  NODE_ENV=development DEBUG=express-mssql-pooling:* nodemon ./src/server.js",
